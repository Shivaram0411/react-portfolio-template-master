{
    "headers": [
        {
        "source": "/manifest.json",
        "headers": [
            { "key": "Content-Type", "value": "application/json" }
        ]
        }
    ],
    "rewrites": [
        { "handle": "filesystem" }, 
        { "source": "/(.*)", "destination": "/index.html" }
    ]
}  
  